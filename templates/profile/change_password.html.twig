{% extends 'base.html.twig' %}

{% block body %}
    <h1>Change Password</h1>

    {{ form_start(form) }}
    {{ form_row(form.oldPassword) }}
    {{ form_row(form.newPassword.first) }}
    {{ form_row(form.newPassword.second) }}

    <button type="submit">Change Password</button>
    {{ form_end(form) }}
{% endblock %}
